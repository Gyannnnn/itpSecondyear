generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Students {
  id                 String     @id       @default(uuid())
  name               String
  gender             String
  email              String     @unique
  registrationNumber String     @unique
  branch             String
  section            String
  whatsappNo         String
  primaryDomain      String
  secondaryDomain    String
  githubUrl          String
  projectLink1       String
  projectLink2       String
  resumeLink         String?
}
